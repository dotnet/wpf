<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>BamlAvoidXmlTest</AssemblyName>
    <RootNamespace>BamlAvoidXmlTest</RootNamespace>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <PropertyGroup>
    <RuntimeIdentifiers>win10-x64;win10-x86</RuntimeIdentifiers>
    <StartupObject>BamlAvoidXmlTest.Entry</StartupObject>
    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <!-- 
      System.Xaml in Microsoft.DotNet.Wpf.DncEng will be superseded by the project reference
    -->
    <ProjectReference Include="$(WpfSourceDir)System.Xaml\System.Xaml.csproj" />
    <PackageReference Include="$(MicrosoftDotNetWpfDncEngPackage)" Version="$(MicrosoftDotNetWpfDncEngVersion)" />
  </ItemGroup>
</Project>
