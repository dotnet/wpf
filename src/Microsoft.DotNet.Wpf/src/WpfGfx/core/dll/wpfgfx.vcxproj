      <MergeSections>%(MergeSections);_PAGE=PAGE;_TEXT_.text</MergeSections>
      <StackReserveSize>0x40000</StackReserveSize>
    </Link>
  </ItemDefinitionGroup>

  <ItemDefinitionGroup>
