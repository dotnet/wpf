  <FontFamily
     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/composite-font"
     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns:System="clr-namespace:System;assembly=mscorlib"
     Baseline="0.9"
     LineSpacing="1.2"
     OS="Windows10RS1">
    <!-- Arial Unicode MS ships with Office, not Windows, but is likely to be on many 
         systems, so making use of that here. -->

    <!-- Name mapping -->
    <FontFamily.FamilyNames>
      <System:String x:Key="en-US">Global User Interface</System:String>
    </FontFamily.FamilyNames>

    <!-- Faces to report in font chooser UI -->
    <FontFamily.FamilyTypefaces>
      <FamilyTypeface
          Weight="Normal" Stretch="Normal" Style="Normal"
          UnderlinePosition="-0.1" UnderlineThickness="0.05"
          StrikethroughPosition="0.3" StrikethroughThickness="0.05"
          CapsHeight="0.5" XHeight="0.3" />

      <FamilyTypeface
          Weight="Bold" Stretch="Normal" Style="Normal"
          UnderlinePosition="-0.1" UnderlineThickness="0.05"
          StrikethroughPosition="0.3"  StrikethroughThickness="0.05"
          CapsHeight="0.5" XHeight="0.3" />
    </FontFamily.FamilyTypefaces>

    <!-- This revision is based on Unicode 9.0 -->

    <!-- Character to family lookups (in lookup order) -->
    <FontFamily.FamilyMaps>

      <!--
            Basic Latin                             0000-007F
            Latin-1 Supplement                      0080-00FF
            Latin Extended-A                        0100-017F
            Latin Extended-B                        0180-024F
            IPA Extensions                          0250-02AF
            Spacing Modifier Letters                02B0-02FF
            Combining Diacritical Marks             0300-036F
            Greek and Coptic                        0370-03FF
            Cyrillic                                0400-04FF 
            Cyrillic Supplement                     0500-052F 
            Hebrew                                  0590-05FF
            Arabic                                  0600-06FF
            Arabic Supplement                       0750-077F - Unicode 4.1, supported in Vista fonts
            Arabic Extended-A                       08A0-08FF - Unicode 6.1, supported in Windows 8.1 fonts
            Cyrillic Extended-C                     1C80-1C8F
            Phonetic Extensions                     1D00-1D7F
            Phonetic Extensions Supplement          1D80-1DBF - Unicode 4.1, supported in Vista fonts
            Combining Diacritical Marks Supplement  1DC0-1DFF - Unicode 4.1, supported in Vista fonts
            Latin Extended Additional               1E00-1EFF
            Greek Extended                          1F00-1FFF
            Latin Extended-C                        2C60-2C7F - Unicode 5.0, supported in Vista fonts
            Latin Extended-D                        A720-A7FF - Unicode 5.0, supported (minimally) in Windows 8 fonts
            Latin Extended-E                        AB30-AB6F
            Cyrillic Extended-A                     2DE0-2DFF - Unicode 5.1, supported in Windows 8 fonts
            Cyrillic Extended-B                     A640-A69F - Unicode 5.1, supported in Windows 8 fonts
            Alpha Pres Forms Latin                  FB00-FB0F
            Alpha Pres Forms (Hebrew)               FB1D-FB4F
            Arabic Pres Forms-A                     FB50-FDCF
            Arabic Pres Forms-A                     FDF0-FDFF
            Combining Half Marks                    FE20-FE2F
            Arabic Pres Forms-B                     FE70-FEFE
            -->
      <!-- Windows 8: removed Uyghur-specific maps -->
      <FontFamilyMap
          Unicode     = "0000-052F, 0590-06FF, 0750-077F, 08A0-08FF, 1C80-1C8F, 1D00-1FFF, 2C60-2C7F, A720-A7FF, AB30-AB6F, FB00-FB0F, FB1D-FBFF"
          Target      = "Segoe UI, Tahoma, Arial, Simplified Arabic, Traditional Arabic, Arial Unicode MS, Microsoft Sans Serif, Lucida Sans Unicode"
          Scale       = "1.0" />
      <FontFamilyMap
          Unicode     = "FC00-FDCF, FDF0-FDFF, FE20-FE2F, FE70-FEFE"
          Target      = "Tahoma, Simplified Arabic, Traditional Arabic, Arial Unicode MS"
          Scale       = "1.0" />

      <!--
            Armenian                    0530-058F    
            Georgian (Khutsuri)         10A0-10CF - supported in Win8
