            <DataTrigger Binding="{Binding Path=(base:AccessibilitySwitches.UseNetFx47CompatibleAccessibilityFeatures)}" Value="false">
                <Setter Property="AutomationProperties.Name"
                      Value="{Binding Path=(AutomationProperties.Name),
                                  Mode=OneWay, 
