<!--
  
  In order to maintain compat with previous versions and take new font fallbacks for Win10 and beyond, we expand 
  this XML to contain a collection of font families demarcated by an OS version attribute.  The values for this 
  attribute are the same as those in the OperatingSystemVersion enumeration defined in OperatingSystemVersion.cs.
  To add a new FontFamily, simple copy the section into the collection and then mark it with OS={ValidOS}.  If 
  the OS does not already exist, please see OperatingSystemVersion.cs for instructions on how to add it.
  
  
  When adding a new font fallback section it should be added as the topmost FontFamily.  This ordering ensures that later OSes
  are checked first while this file is being parsed.
  
  
  Adding RS5 CompositeFont definitions
-->

<FontFamilyCollection
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/composite-font">

  <FontFamily
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/composite-font"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    Baseline="0.9"
    LineSpacing="1.2"
    OS="Windows10RS5">

        <!-- Name mapping -->
        <FontFamily.FamilyNames>
            <System:String x:Key="en-US">Global User Interface</System:String>
        </FontFamily.FamilyNames>

        <!-- Faces to report in font chooser UI -->
        <FontFamily.FamilyTypefaces>
            <FamilyTypeface
                Weight="Normal" Stretch="Normal" Style="Normal"
                UnderlinePosition="-0.1" UnderlineThickness="0.05"
                StrikethroughPosition="0.3" StrikethroughThickness="0.05"
                CapsHeight="0.5" XHeight="0.3" />

            <FamilyTypeface
                Weight="Bold" Stretch="Normal" Style="Normal"
                UnderlinePosition="-0.1" UnderlineThickness="0.05"
                StrikethroughPosition="0.3"  StrikethroughThickness="0.05"
                CapsHeight="0.5" XHeight="0.3" />
        </FontFamily.FamilyTypefaces>

        <!-- This revision is based on Unicode 11.0 -->

        <!-- Historical notes:

         Previous versions used Arial Unicode MS, which never shipped in Windows, but was bundled with Office and included 
         on the assumption that it would often be present. Office stopped shipping it some time ago, so it has been removed.

         Previous versions used Arial and Lucida Sans Unicode. The assigned characters covered by those font are entirely
         covered by Segoe UI + Segoe UI Symbol, so they have been removed.

         Some historical scripts were added in Segoe UI Symbol in Windows 7, Windows 8 or Windows 8.1:
         - Windows 7: Ogham, Runic
         - Windows 8: Glagolitic, Gothic, Old Italic, Old Turkic
         - Windows 8.1: Coptic, Meroitic Cursive 
         Later, in Windows 10 TH1, these were removed from that font and put into a new Segoe UI Historic font. Ranges
         for these scripts have used both fonts to make the data portable to Windows 8.

         -->

        <!-- Character to family lookups (in lookup order) -->
        <FontFamily.FamilyMaps>

            <!--
            Basic Latin                             0000-007F
            Latin-1 Supplement                      0080-00FF
            Latin Extended-A                        0100-017F
            Latin Extended-B                        0180-024F
            IPA Extensions                          0250-02AF
            Spacing Modifier Letters                02B0-02FF
            Combining Diacritical Marks             0300-036F
            Greek and Coptic                        0370-03FF
            Cyrillic                                0400-04FF 
            Cyrillic Supplement                     0500-052F - Unicode 3.2+
            Hebrew                                  0590-05FF - Missing 05EF (new in Unicode 11)
            Combining Diacritical Marks Extended    1AB0-1AFF - Unicode 7.0, not supported in any fonts
            Cyrillic Extended-C                     1C80-1C8F - Unicode 9.0, not supported in any fonts
            Phonetic Extensions                     1D00-1D7F - Unicode 4.0
            Phonetic Extensions Supplement          1D80-1DBF - Unicode 4.1
            Combining Diacritical Marks Supplement  1DC0-1DFF - Unicode 4.1 - 10; of 63 assigned characters, 13 covered by Segoe UI, 1 covered by Ebrima
            Latin Extended Additional               1E00-1EFF
            Greek Extended                          1F00-1FFF
            Latin Extended-C                        2C60-2C7F - Unicode 5.0
            Cyrillic Extended-A                     2DE0-2DFF - Unicode 5.1
            Cyrillic Extended-B                     A640-A69F - Unicode 5.1
            Latin Extended-D                        A720-A7FF - Unicode 5.0 - 11; missing A7AF, A7B8 and A7B9 (new in Unicode 11)
            Latin Extended-E                        AB30-AB6F - Unicode 7 - 8
            Alphabetic Presentation Forms (Latin)   FB00-FB0F
                (Alphabetic Presentation Forms (Georgian), FB10-FB1C: see below, by 0530-058F)
            Alphabetic Presentation Forms (Hebrew)  FB1D-FB4F
            Combining Half Marks                    FE20-FE2F - Partially covered by Segoe UI + Segoe UI Symbol
            -->
            <!-- Above ranges covered by Segoe UI except where noted. -->
            <FontFamilyMap
                Unicode     = "0000-052F, 0590-05FF, 1AB0-1AFF, 1C80-1C8F, 1D00-1FFF, 2C60-2C7F, 2DE0-2DFF, A640-A69F, A720-A7FF, AB30-AB6F, FB00-FB0F, FB1D-FBFF, FE20-FE2E"
                Target      = "Segoe UI, Segoe UI Symbol, Ebrima"
                Scale       = "1.0" />


            <!--
            Armenian                                0530-058F - missing 0560, 0588 (new in Unicode 11)
            Alpha Pres Forms (Armenian)             FB10-FB1C
            -->
            <FontFamilyMap
                Unicode     = "0530-058F, FB10-FB1C"
                Target      = "Segoe UI"
                Scale       = "1.0" />


            <!-- Hebrew (0590-05FF): see above, by 0000-007F -->


            <!--
            Arabic                                  0600-06FF
            Arabic Supplement                       0750-077F - Unicode 4.1, supported in Vista fonts
            Arabic Extended-A                       08A0-08FF - Unicode 6.1, supported in Windows 8.1 fonts
            Arabic Presentation Forms-A             FB50-FDCF, FDF0-FDFF (non-characters FDD0-FDEF are ignored)
                                                              - 611 assigned characters. 178 covered by Segoe UI. Remainder requires Simplified Arabic.
            Arabic Presentation Forms-B             FE70-FEFE - Fully covered by Segoe UI
        -->
            <FontFamilyMap
                Unicode     = "0600-06FF, 0750-077F, 08A0-08FF, FB50-FDCF, FDF0-FDFF, FE70-FEFE"
                Target      = "Segoe UI, Simplified Arabic"
                Scale       = "1.0" />


            <!-- 
            Syriac                      0700-074F
            Syriac Supplement           0860-086F
            -->
            <FontFamilyMap
                Unicode     = "0700-074F, 0860-086F"
                Target      = "Segoe UI Historic"
                Scale       = "1.0" />

            <!-- Arabic Supplement (0750-077F): see above, by 0600-06FF -->

            <!-- Thaana                      0780-07BF -->
            <FontFamilyMap
                Unicode     = "0780-07BF"
                Target      = "MV Boli"
                Scale       = "1.0" />

