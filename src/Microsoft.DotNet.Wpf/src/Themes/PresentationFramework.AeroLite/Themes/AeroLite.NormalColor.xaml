    <MenuScrollingVisibilityConverter x:Key="&#303;"/>
    <Geometry x:Key="&#203;"> M 0,0 L 3.5,4 L 7,0 Z</Geometry>
    <Geometry x:Key="&#304;">   M 0,4 L 3.5,0 L 7,4 Z</Geometry>
    <Geometry x:Key="&#305;">M 0,0 L 4,3.5 L 0,7 Z</Geometry>


