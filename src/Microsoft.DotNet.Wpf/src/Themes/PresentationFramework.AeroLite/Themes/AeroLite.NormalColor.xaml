                            <Setter Property="Fill" Value="{StaticResource &#230;}" TargetName="indeterminateMark" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Opacity" Value="0.56" TargetName="templateRoot" />
